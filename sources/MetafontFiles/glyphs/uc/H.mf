% box dimension definition %
beginadjchar("H",5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.35u#,space*0.35u#);

% construction %
x1=x2=x3=leftstemloc;
x4=x5=x6=w-x1;
y3=y4=h;
y1=y6=0;
y5=y2=bar3;

% draw %
draw z1--z3;
draw z6--z4;
draw z2--z5;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+bar2);

penlabels(range 0 thru 30);

endchar;
