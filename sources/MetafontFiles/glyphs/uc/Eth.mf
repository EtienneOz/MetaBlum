% box dimension definition %
beginadjchar(208,8,cap#,0);

% sidebearings %
if (sidebearing=1):
numeric l,r;
normal_adjust_fit(space*0.05u#,space*-0.2u#);fi

% construction %
x1=x2=0.2w;
x4=x5=w-x1;
x3=x6;
x7=leftstemloc;
x8=0.4w;
y2=y3=h;
y1=y6=0;
y4=bar5;
y5=bar1;
y7=y8=bar3;
z6=z5+whatever*dir(ang);

% draw %
draw z1--z2--z3--z4--z5--z6--cycle;
draw z7--z8;

penlabels(range 0 thru 30);

endchar;