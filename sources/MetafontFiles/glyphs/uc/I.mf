% box dimension definition %
beginadjchar("I",1.5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.1u#,space*0.1u#);

% construction %
x1=x2=.5w;
x3=x5=leftstemloc;
x4=x6=w;
y2=h;
y1=0;
y3=y4=h;
y5=y6=0;

% draw %
draw z1--z2;
draw z3--z4;
draw z5--z6;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+bar2);

penlabels(range 0 thru 30);

endchar;
