% box dimension definition %
beginadjchar("D",5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.35u#,space*0.3u#);

% construction %
x1=x2=leftstemloc;
x3=w;
y1=leftstemloc;
y2=h;
y3=bar3;

% draw %
draw z1--z2;
draw z2..z3..z1;

% composite glyphs %
charanchortops_[charcode]=(.45w,h+bar2);

penlabels(range 0 thru 30);

endchar;
