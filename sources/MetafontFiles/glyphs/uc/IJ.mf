% box dimension definition %
beginadjchar(156,7,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.35u#,space*-0.15u#);

% construction %
x1=0.2w;
x6=x7=leftstemloc;
x4=x5=w-x1;
y5=y7=h;
y2=y3=y6=0;
y1=y4=bar1;
z2=z1+whatever*dir(-ang);   
z3=z4+whatever*dir(ang);

% draw %
draw z1--z2--z3--z4--z5;
draw z6--z7;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+bar2);

penlabels(range 0 thru 30);

endchar;