% box dimension definition %
beginadjchar("L",5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.35u#,space*0.1u#);

% construction %
x1=x2=leftstemloc;
x3=w-x1;
y2=h;
y1=y3=0;

% draw %
draw z3--z1--z2;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+bar2);

penlabels(range 0 thru 30);

endchar;