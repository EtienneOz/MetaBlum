% box dimension definition %
beginadjchar("W",7,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.15u#,space*0.15u#);

% construction %
x1=leftstemloc;
x5=w-x1;
x2=.25w;
x3=.5w;
x4=.75w;
y1=y5=h;
