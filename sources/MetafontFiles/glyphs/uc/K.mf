% box dimension definition %
beginadjchar("K",5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.35u#,space*0.1u#);

% construction %
x1=x2=x3=leftstemloc;
x5=x4=x6=w-x1;
y3=y4=h;
y1=y6=0;
y2=bar3;

% draw %
draw z1--z3;
draw z2--z6;
draw z2--z4;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+bar2);

penlabels(range 0 thru 30);

endchar;