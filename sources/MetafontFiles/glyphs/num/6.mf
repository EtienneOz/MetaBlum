% construction %
x4=x5=x6=leftstemloc;
x1=x9=x10=w-x4;
x2=x8=x11;
x3=x7=x12;
y3=y2=h;
y7=y8=0;
y6=y9=bar1;
y5=y10=bar2;
y12=y11=bar3;
y4=y1=bar5;
z3=z4+whatever*dir(ang); 
z2=z1+whatever*dir(-ang); 

% draw %
draw z1--z2--z3--z4--z5--z6--z7--z8--z9--z10--z11--z12--z5;

penlabels(range 0 thru 30);

endchar;