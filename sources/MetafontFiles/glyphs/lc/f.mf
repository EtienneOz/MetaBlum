% box dimension definition %
beginadjchar("f",1,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.3u#,space*0.2u#);

% construction %
x2=x3=x4=leftstemloc;
x5=x1;
y1=h;
y2=bar5;
y4=y5=bar3;
y3=0;
z1=z2+whatever*dir(ang);

% draw %
draw z1--z2--z3;
draw z4--z5;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+1/6cap);

penlabels(range 0 thru 30);

endchar;