% box dimension definition %
beginadjchar(158,5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.25u#,space*0.35u#);
 
% construction %
x6=x7=leftstemloc;
x1=x2=x3=x10=w-x6;
x5=x8;
x4=x9;
x13=.6w;
x12=1.1w;
y1=h;
y2=y8=y9=0;
y10=y7=bar1;
y3=y6=bar2;
y4=y5=bar3;
y12=y13=bar5;
z5=z6+whatever*dir(ang); 
z9=z10+whatever*dir(ang); 

% draw %
draw z3--z4--z5--z6--z7--z8--z9--z10;
draw z1--z2;
draw z12--z13;

penlabels(range 0 thru 30);

endchar;