% box dimension definition %
beginadjchar("t",3,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.1u#,space*0.1u#);

% construction %
x4=leftstemloc;
x5=w-x4;
x2=x3=.5w;
y2=h;
y3=0;
y4=y5=bar3;

% draw %
draw z2--z3;
draw z4--z5;

% composite glyphs %
charanchortops_[charcode]=(w,h);
charanchorbots_[charcode]=(.5w,0);

penlabels(range 0 thru 30);

endchar;