% box dimension definition %
beginadjchar(173,5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.3u#,space*0.25u#);

% construction %
x1=x2=x3=leftstemloc;
x6=x7=w-x1; 
y2=0;
y3=y6=bar2;
y1=y4=y5=bar3;
y7=-bar2;
y8=-bar3;
z3=z4+whatever*dir(ang); 
z5=z6+whatever*dir(-ang);
z8=z7+whatever*dir(ang);

% draw %
draw z2--z3--z4--z5--z6--z7--z8;
draw z1--z2;

penlabels(range 0 thru 30);

endchar;