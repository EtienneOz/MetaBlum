% box dimension definition %
beginadjchar(248,5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.25u#,space*0.25u#);

% construction %
x4=x5=x9=leftstemloc;
x1=x8=x10=w-x4;
x3=x6;
x2=x7;
y6=y7=0;
y5=y8=bar1;
y1=y4=bar2;
y2=y3=bar3;
y9=-bar1;
y10=bar4;
z6=z5+whatever*dir(-ang); 
z7=z8+whatever*dir(ang); 

% draw %
draw z1--z2--z3--z4--z5--z6--z7--z8--cycle;
draw z9--z10;

penlabels(range 0 thru 30);

endchar;