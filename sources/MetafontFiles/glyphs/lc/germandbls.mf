% box dimension definition %
beginadjchar(255,5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.3u#,space*0.25u#);

% construction %
x1=x2=leftstemloc;
x4=x7=x10;
x5=x6=x9=x8=w-x1;
x11=x3;
y1=y10=y11=0;
y3=y4=h;
y9=bar1;
y8=bar2;
y2=bar5;
y7=bar3;
y6=bar4;
y5=bar5;
z7=z6+whatever*dir(ang);
z3=z2+whatever*dir(ang);  

% draw %
draw z1--z2--z3--z4--z5--z6--z7--z8--z9--z10--z11;

penlabels(range 0 thru 30);

endchar;


