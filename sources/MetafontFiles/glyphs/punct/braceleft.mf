% construction %
x5=leftstemloc;
x9=x3;
x1=x2=x4=x10;
y5=2.5bar1;
y1=bar2;
y2=bar3;
y9=h;
y3=-bar1;
y4=0;
y10=bar5; 
z3=z4+whatever*dir(-ang);
z1=z5+whatever*dir(-ang);

% draw %
draw z9--z10--z2--z5--z1--z4--z3;

penlabels(range 0 thru 30);

endchar;