% box dimension definition %
beginadjchar(159,5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.2u#,space*0.2u#);

% construction %
x4=x5=x11=leftstemloc;
x1=x8=x14=w-x4;
x3=x6=x10=x12;
x2=x7=x9=x13;
y6=y7=0;
y5=y8=bar1;
y9=y10=bar2;
y1=y4=bar3;
y3=y2=bar4;
y11=y14=bar5;
y12=y13=h;
z6=z5+whatever*dir(-ang); 
z7=z8+whatever*dir(ang); 

% draw %
draw z5--z6--z7--z8--z9--z10--z4--z3;
draw z9--z1--z2--z3--z11--z12--z13--z14;

penlabels(range 0 thru 30);

endchar;